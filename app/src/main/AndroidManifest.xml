<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.CommonRoom"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login"/>
        <activity android:name=".SignUp"/>
        <activity android:name=".Access"/>
        <activity android:name=".UserProfileActivity"/>
        <activity android:name=".EditUserActivity"/>
        <activity android:name=".DormSelection"/>
        <activity android:name=".Choice"/>
        <activity android:name=".PersResActivity"/>
        <activity android:name=".AvailRoomsActivity"/>
        <activity android:name=".DeleteDorm" />
        <activity android:name=".TestHomePage"/>
    </application>

</manifest>